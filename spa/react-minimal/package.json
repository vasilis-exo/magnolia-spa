{
  "name": "react-minimal",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@magnolia/react-renderer": "../../../frontend-helpers/packages/react-renderer/",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0"
  },
  "scripts": {
    "start": "env-cmd -f .env.local react-scripts start",
    "build:mgnl": "npm run clean && env-cmd -f .env.mgnl react-scripts build --env=development",
    "deploy:mgnl": "npm run build:mgnl && copyfiles -u 1 \"build/**/*\" ../../magnolia/light-modules/react-minimal-lm/webresources/build",
    "clean": "rimraf build && rimraf ../../magnolia/light-modules/spa-lm/webresources/build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "copyfiles": "^2.1.1",
    "env-cmd": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "rimraf": "^3.0.0"
  }
}
