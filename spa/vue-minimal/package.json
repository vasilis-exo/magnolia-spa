{
  "name": "vue-minimal",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy:mgnl": "npm run clean && npm run build && copyfiles -u 1 \"dist/**/*\" ../../magnolia/light-modules/vue-minimal-lm/webresources/dist",
    "clean": "rimraf dist && rimraf ../../magnolia/light-modules/vue-minimal-lm/webresources/dist"
  },
  "dependencies": {
    "@magnolia/vue-editor": "1.3.3",
    "core-js": "^3.30.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.47",
    "copyfiles": "^2.4.1",
    "eslint": "^8.39.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^9.11.0",
    "node-sass": "^9.0.0",
    "rimraf": "^5.0.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
